  },
  "homepage": "https://github.com/karissa/dat-app#readme",
  "dependencies": {
    "dat": "^9.7.0",
    "dat-manager": "^1.3.0",
    "debug": "^2.2.0",
    "drag-and-drop-files": "0.0.1",
    "electron-packager": "^5.2.0",
    "electron-prebuilt": "^0.35.5",
    "electron-rpc": "^1.0.3",
    "menubar": "^2.3.0",
    "mkdirp": "^0.5.1",
    "node-notifier": "^4.4.0",
    "os-homedir": "^1.0.1",
